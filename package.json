{
  "name": "tailwindcss-flow",
  "version": "0.0.6",
  "description": "Is a plugin designed to improve productivity in application development by providing additional utilities.",
  "main": "src/index.ts",
  "types": "src/index.d.ts",
  "scripts": {
    "build:ts": "tsc",
    "build:css": "npx tailwindcss -i src/utilities.css -o dist/tailwindcss.css",
    "build": "tsc && npm run build:css"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/halvaradop/tailwindcss-flow.git"
  },
  "keywords": [
    "tailwind",
    "tailwindcss",
    "tailwind plugin",
    "utilities",
    "productivity",
    "web",
    "development"
  ],
  "author": "Hernan Alvarado <hernanvid123@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/halvaradop/tailwindcss-flow/issues"
  },
  "homepage": "https://github.com/halvaradop/tailwindcss-flow#readme",
  "devDependencies": {
    "autoprefixer": "^10.4.19",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.3"
  },
  "peerDependencies": {
    "tailwindcss": ">=3.0.0"
  }
}
